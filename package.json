    "test": "tests"
  },
  "scripts": {
    "restore": "rm -f db/topsify.sqlite && ls db/restore/*.sql | sort | xargs cat | sqlite3 db/topsify.sqlite",
    "restore:test": "rm -f test/test.sqlite && ls db/restore/*.sql | sort | xargs cat | sqlite3 test/test.sqlite",
    "test": "mocha",
    "test:1a": "mocha test/1a.test.js",
    "test:1b": "mocha test/1b.test.js",
    "test:2a": "mocha test/2a.test.js",
    "test:2b": "mocha test/2b.test.js",
    "test:3a": "mocha test/3a.test.js",
    "test:3b": "mocha test/3b.test.js",
    "test:4a": "mocha test/4a.test.js",
    "test:4b": "mocha test/4b.test.js",
    "test:5a": "mocha test/5a.test.js",
    "test:5b": "mocha test/5b.test.js",
    "test:6a": "mocha test/6a.test.js",
    "test:6b": "mocha test/6b.test.js",
    "test:7a": "mocha test/7a.test.js",
    "test:7b": "mocha test/7b.test.js",
    "test:8a": "mocha test/8a.test.js",
    "test:8b": "mocha test/8b.test.js",
